<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:local="http://schemas.android.com/apk/res-auto"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/relativeLayout1">
    <ScrollView
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:background="@drawable/background"
        p1:id="@+id/horizontalScrollView1">
        <LinearLayout
            p1:orientation="vertical"
            p1:layout_width="match_parent"
            p1:layout_height="match_parent"
            p1:id="@+id/linearLayout1"
            p1:gravity="center">
            <ImageView
                p1:src="@drawable/logo_03"
                p1:layout_width="match_parent"
                p1:layout_height="142.5dp"
                p1:id="@+id/imageView1" />
            <TextView
                p1:text="Username"
                p1:textAppearance="?android:attr/textAppearanceLarge"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/textView1"
                p1:layout_marginLeft="25dip"
                p1:layout_marginRight="25dip" />
            <EditText
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/inputUsername"
                p1:layout_marginLeft="25dip"
                local:MvxBind="Text Username"
                p1:layout_marginRight="25dip" />
            <TextView
                p1:text="Password"
                p1:textAppearance="?android:attr/textAppearanceLarge"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/inputPassword"
                p1:layout_marginLeft="25dip"
                p1:layout_marginRight="25dip" />
            <EditText
                p1:inputType="textPassword"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                local:MvxBind="Text Password"
                p1:id="@+id/inputPassword"
                p1:layout_marginLeft="25dip"
                p1:layout_marginRight="25dip" />
            <Button
                p1:text="Login"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/buttonLogin"
                p1:layout_marginLeft="25dip"
                p1:layout_marginRight="25dip"
                local:MvxBind="Click LoginCommand" />
        </LinearLayout>
    </ScrollView>
</RelativeLayout>