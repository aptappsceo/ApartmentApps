@model ApartmentApps.Data.MaitenanceRequest

@{
    ViewBag.Title = "Details";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Details</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("List", "Index")
            </li>
            <li class="active">
                <strong>Details</strong>
            </li>
        </ol>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Maitenance Request Details</h5>
                </div>
                <div class="ibox-content">
                    <dl class="dl-horizontal">
                        <dt>
                            Request Type
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.MaitenanceRequestType.Name)
                        </dd>

                        <dt>
                            Status
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Status.Name)
                        </dd>

                        <dt>
                            Unit
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Unit.Name)
                        </dd>

                        <dt>
                            Requested By
                        </dt>

                        <dd>
                            @Model.User.FirstName @Model.User.LastName ( @Model.User.Email )
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.PermissionToEnter)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PermissionToEnter)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.PetStatus)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.PetStatus)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.ScheduleDate)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.ScheduleDate)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => model.Message)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => model.Message)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.SubmissionDate)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.SubmissionDate)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.CompletionDate)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CompletionDate)
                        </dd>

                    </dl>
                    @*@Html.ActionLink("Edit", "Edit", new {id = Model.Id}, new {@class = "btn btn-primary"})
                    @Html.ActionLink("Back to List", "Index", null, new {@class = "btn btn-white"})*@

                </div>
                <div class="ibox-content inspinia-timeline">
                    @foreach (var checkin in Model.Checkins.OrderBy(p => p.Date))
                    {
                        <div class="timeline-item">
                            <div class="row">
                                <div class="col-xs-3 date">
                                    <i class="fa fa-briefcase"></i>
                                    @checkin.Date.ToString()
                                    <br>
                                    @*<small class="text-navy">2 hour ago</small>*@
                                </div>
                                <div class="col-xs-7 content no-top-border">
                                    <p class="m-b-xs"><strong>@checkin.StatusId</strong></p>

                                    <p>
                                        @checkin.Comments
                                    </p>

                                    @*<p><span data-diameter="40" class="updating-chart" style="display: none;">7,3,5,2,5,4,9,3,4,2,2,1,0,9,4,8,2,8,10,6,1,3,3,5,8,1,5,4,5,2,8,6,9,2,7,1,8</span><svg class="peity" height="16" width="64"><polygon fill="#1ab394" points="0 15 0 5 1.7777777777777777 11 3.5555555555555554 8 5.333333333333333 12.5 7.111111111111111 8 8.88888888888889 9.5 10.666666666666666 2 12.444444444444443 11 14.222222222222221 9.5 16 12.5 17.77777777777778 12.5 19.555555555555554 14 21.333333333333332 15.5 23.11111111111111 2 24.888888888888886 9.5 26.666666666666664 3.5 28.444444444444443 12.5 30.22222222222222 3.5 32 0.5 33.77777777777778 6.5 35.55555555555556 14 37.33333333333333 11 39.11111111111111 11 40.888888888888886 8 42.666666666666664 3.5 44.44444444444444 14 46.22222222222222 8 48 9.5 49.77777777777777 8 51.55555555555555 12.5 53.33333333333333 3.5 55.11111111111111 6.5 56.888888888888886 2 58.666666666666664 12.5 60.44444444444444 5 62.22222222222222 14 64 3.5 64 15"></polygon><polyline fill="transparent" points="0 5 1.7777777777777777 11 3.5555555555555554 8 5.333333333333333 12.5 7.111111111111111 8 8.88888888888889 9.5 10.666666666666666 2 12.444444444444443 11 14.222222222222221 9.5 16 12.5 17.77777777777778 12.5 19.555555555555554 14 21.333333333333332 15.5 23.11111111111111 2 24.888888888888886 9.5 26.666666666666664 3.5 28.444444444444443 12.5 30.22222222222222 3.5 32 0.5 33.77777777777778 6.5 35.55555555555556 14 37.33333333333333 11 39.11111111111111 11 40.888888888888886 8 42.666666666666664 3.5 44.44444444444444 14 46.22222222222222 8 48 9.5 49.77777777777777 8 51.55555555555555 12.5 53.33333333333333 3.5 55.11111111111111 6.5 56.888888888888886 2 58.666666666666664 12.5 60.44444444444444 5 62.22222222222222 14 64 3.5" stroke="#169c81" stroke-width="1" stroke-linecap="square"></polyline></svg></p>*@
                                </div>
                            </div>
                        </div>

                    }



                </div>
            </div>
        </div>
    </div>
</div>












