@using ApartmentApps.Api.Modules
@using ApartmentApps.Data
@model dynamic
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Dashboard</h2>
        <ol class="breadcrumb">
            <li class="active">
                @*<strong>@Model.StartDate.Value.ToShortDateString() - @Model.EndDate.Value.ToShortDateString()</strong>*@
            </li>
        </ol>
    </div>
</div>
<br />
<div class="row">
    <div class="col-md-6">
        @Html.RenderDashboardArea(DashboardArea.LeftTop)
        @Html.RenderDashboardArea(DashboardArea.Left)
        
        <div class="clear-fix"></div>
    </div>
    <div class="col-md-6">
        @Html.RenderDashboardArea(DashboardArea.RightTop)
        @Html.RenderDashboardArea(DashboardArea.Right)

        <div class="clear-fix"></div>
    </div>
</div>