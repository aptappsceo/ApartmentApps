<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Apartment Apps Management Portal | @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @*@Styles.Render("~/font-awesome/css") *@

    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1" crossorigin="anonymous">

    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/ej/web/bootstrap-theme/ej.web.all.min.css")" rel="stylesheet" />
    <link href="/Content/FormFactory/FormFactory.css" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/AA.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/plugins/dataTables/datatables.min.css")" rel="stylesheet" type="text/css"/>

    <link href="@Url.Content("~/Content/easyquery.css")" rel="stylesheet" type="text/css" media="screen" />
    <link href="@Url.Content("~/Content/eqview.css")" rel="stylesheet" type="text/css" media="screen" />
    @RenderSection("csspage", required: false)

</head>
<body>

    <!-- Wrapper-->
    <div id="wrapper">
        <!-- Navigation -->
        @Html.Partial("_Navigation")

        <!-- Page wraper -->
        <div id="page-wrapper" class="gray-bg">

            <!-- Top Navbar -->
            @Html.Partial("_TopNavbar")

            <!-- Main view  -->
            @RenderBody()

            <!-- Footer -->
            @Html.Partial("_Footer")

        </div>
        <!-- End page wrapper-->

    </div>
    <!-- End wrapper-->

    @Scripts.Render("~/bundles/jquery")
    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"></script>
    @*<script src="jquery.min.js" type="text/javascript"></script>
    <script src="jquery.timeago.js" type="text/javascript"></script>*@
    <script>
        jQuery(document).ready(function () {
            jQuery("abbr.timeago").timeago();
        });
    </script>

    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/plugins/metsiMenu")
    @Scripts.Render("~/plugins/pace")
    @Scripts.Render("~/plugins/flot")
    @Scripts.Render("~/plugins/slimScroll")

    <script type="text/javascript" src="https://www.google.com/jsapi"></script>
    @Html.EJ().ScriptManager()
    
    @RenderSection("scripts", required: false)
    <script src="@Url.Content("~/Scripts/eq.all.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/eq.view.basic.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/eq.view.grid.js")" type="text/javascript"></script>
   
    <script src="/Scripts/FormFactory/FormFactory.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/gridmvc.min.js")" type="text/javascript"> </script>
    @Scripts.Render("~/bundles/inspinia")
    <script src="@Url.Content("~/Scripts/app/aa.js")" type="text/javascript"> </script>
</body>
</html>
