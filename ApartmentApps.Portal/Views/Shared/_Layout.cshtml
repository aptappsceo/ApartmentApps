<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Apartment Apps Management Portal | @ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @*@Styles.Render("~/font-awesome/css") *@

    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1" crossorigin="anonymous">


    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/ej/web/bootstrap-theme/ej.web.all.min.css")" rel="stylesheet"/>
    <link href="/Content/FormFactory/FormFactory.css" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/AA.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/plugins/dataTables/datatables.min.css")" rel="stylesheet" type="text/css"/>

    <link href="@Url.Content("~/Content/easyquery.css")" rel="stylesheet" type="text/css" media="screen"/>
    <link href="@Url.Content("~/Content/eqview.css")" rel="stylesheet" type="text/css" media="screen"/>
    <link href="@Url.Content("~/Content/plugins/toastr/toastr.min.css")" rel="stylesheet" type="text/css" media="screen"/>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/js/bootstrap.min.js"></script>
    @Scripts.Render("~/bundles/jquery")

    @RenderSection("csspage", required: false)

</head>
<body class="md-skin">

<!-- Wrapper-->
<div id="wrapper">
    <!-- Navigation -->
    @Html.Partial("_Navigation")

    <!-- Page wraper -->
    <div id="page-wrapper" class="gray-bg">

        <!-- Top Navbar -->
        @Html.Partial("_TopNavbar")

        <!-- Main view -->
        @RenderBody()

        <!-- Footer -->
        @Html.Partial("_Footer")

    </div>
    <!-- End page wrapper-->

</div>
<!-- End wrapper-->
<div class="modal inmodal" id="modal" tabindex="-1" role="dialog" aria-hidden="true" data-backdrop="true">
    <div class="modal-dialog">
        <div class="modal-content animated bounceInRight">

        </div>
    </div>
</div>


<script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"></script>
@*<script src="jquery.min.js" type="text/javascript"></script>
    <script src="jquery.timeago.js" type="text/javascript"></script>*@

<script src="@Url.Content("~/Scripts/arrive.js")" type="text/javascript"> </script>
<script>

    $(document).arrive("abbr.timeago", function() {
        // 'this' refers to the newly created element
        jQuery(this).timeago();
    });

    jQuery(document).ready(function() {
        jQuery("abbr.timeago").timeago();
    });

</script>

<!-- Select2 library https://select2.github.io/ -->
<link href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/css/select2.min.css" rel="stylesheet"/>
<script src="https://cdnjs.cloudflare.com/ajax/libs/select2/4.0.3/js/select2.min.js"></script>


<!-- iCheck library https://github.com/fronteed/icheck -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/iCheck/1.0.2/skins/flat/blue.css"/>
<script src="https://cdnjs.cloudflare.com/ajax/libs/iCheck/1.0.2/icheck.min.js"></script>


<!-- Bootstrap Touchspin (Spinner for forms) http://www.virtuosoft.eu/code/bootstrap-touchspin/ -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-touchspin/3.1.2/jquery.bootstrap-touchspin.min.css" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-touchspin/3.1.2/jquery.bootstrap-touchspin.min.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-price-format/2.1/jquery.priceformat.min.js"></script>

    <!-- JQuery Mask Plugin https://igorescobar.github.io/jQuery-Mask-Plugin/
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.0/jquery.mask.min.js"></script>
    -->
    <!-- jasny bootstrap library http://www.jasny.net/bootstrap/
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/css/jasny-bootstrap.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jasny-bootstrap/3.1.3/js/jasny-bootstrap.min.js"></script>
    -->


@Scripts.Render("~/bundles/bootstrap")
@Scripts.Render("~/plugins/metsiMenu")
@Scripts.Render("~/plugins/pace")
@Scripts.Render("~/plugins/flot")
@Scripts.Render("~/plugins/slimScroll")

<script type="text/javascript" src="https://www.google.com/jsapi"></script>
@Html.EJ().ScriptManager()

@RenderSection("scripts", required: false)
<script src="@Url.Content("~/Scripts/eq.all.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/eq.view.basic.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/eq.view.grid.js")" type="text/javascript"></script>

    <!--Override EQ loading bar and result panel height-->
<style>

    .result-panel.loader {
        background: none;
        width: 30px;
        height: 30px;
        background-color: #1ab394;
        margin: 0 auto;
        -webkit-animation: sk-rotatePlane 1.2s infinite ease-in-out;
        animation: sk-rotatePlane 1.2s infinite ease-in-out;
    }

    #ResultPanel { min-height: 300px }
</style>

<script src="/Scripts/FormFactory/FormFactory.js" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/plugins/toastr/toastr.min.js")" type="text/javascript"> </script>
<script src="@Url.Content("~/Scripts/gridmvc.min.js")" type="text/javascript"> </script>
@Scripts.Render("~/bundles/inspinia")
<script src="@Url.Content("~/Scripts/app/aa.js")" type="text/javascript"> </script>

 @if (ViewBag.SuccessMessage != null)
 {
     <script>
         $(function() {
             toastr["success"]('@(ViewBag.SuccessMessage)');
         });
     </script>
    
 }
    @if (ViewBag.ErrorMessage != null)
    {
        <script>
         $(function() {
             toastr["error"]('@(ViewBag.ErrorMessage)');
         });
        </script>

    }
    @if (ViewBag.InfoMessage != null)
    {
        <script>
         $(function() {
             toastr["info"]('@(ViewBag.InfoMessage)');
         });
        </script>

    }
<!-- Bootstrap Datepicker https://eonasdan.github.io/bootstrap-datetimepicker/Options/ -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.43/css/bootstrap-datetimepicker.min.css"/>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.15.2/moment.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.43/js/bootstrap-datetimepicker.min.js"></script>


</body>
</html>