@model ApartmentApps.Api.Modules.DashboardStatViewModel


        <div class="ibox float-e-margins">
            <div class="ibox-title">

                <h5>@Model.Title</h5>
            </div>
            <div class="ibox-content">
                <div class="row">
                    <div class="col-md-6">
                        <h1 class="no-margins">@Model.Value</h1>
                        <div class="stat-percent font-bold text-success">

                        </div>
                        <small>@Model.Subtitle</small>
                    </div>
                </div>
            </div>
        </div>
    